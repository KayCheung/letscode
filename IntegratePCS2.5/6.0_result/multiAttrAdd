#!/bin/sh
#
#  Script:			multiAttrAdd
#  Instance:		1
#  %version:		2 %
#  Description:		
#  %created_by:		buildmgr %
#  %date_created:	Thu Feb 17 17:57:07 2005 %

. ./xroads_config.dat

# Designate branch to search
SEARCH_BASE=$2
export SEARCH_BASE

# Designate entry to modify, make sure it matches SEARCH_BASE
SEARCH_FILTER=$1
export SEARCH_FILTER

# Designate the attribute to modify with an additional value
NEW_ATTR_NAME=$3
export NEW_ATTR_NAME

# Designate the new value
NEW_ATTR_VALUE=$4
export NEW_ATTR_VALUE
./ldapMultiAttrAdd.exe $5

