	and c.rsrc_id = rrr.rsrc_id1
	and rrr.rsrc_id2 = 'VisSvc' and rrr.rel_typ_cd = 'Product' and rrr.rsrc_typ_cd = '2' )) 
	and rsrc_id = 'VisSvc' and rsrc_typ_cd = '1';">>$1.grantPermission.sql;

