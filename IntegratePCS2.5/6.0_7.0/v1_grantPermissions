	java multiAttrDel "(permissionid=$newPerm1^SUBSCRIBER^R)" "ou=usergroup,o=crossroads" permissionid "$newPerm1^SUBSCRIBER^R" 1>>grant$1.out 2>>grant$1.err;
	
	
