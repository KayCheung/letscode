	and c.rsrc_typ_cd = '2');">>$1-$companyId.grantPermission.sql;

	echo "insert into access_auth (accessor_id, rsrc_id, scrty_acc_cd, created_dt , modify_dt, created_by,modify_by,rsrc_typ_cd) 
