echo "Step 2 completed">> grant$1.err;
./multiAttrAdd "(&(permissionid=VRH_Top10*^R)(!(permissionid=VRH_Top10_Std^VisSvc^R)))" "ou=people,o=crossroads" permissionid "VRH_Top10_Std^VisSvc^R" 1>>grant$1.out 2>>grant$1.err;
