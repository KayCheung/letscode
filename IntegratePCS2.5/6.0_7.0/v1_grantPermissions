echo "insert into access_auth (accessor_id, rsrc_id, scrty_acc_cd, created_by,modify_by,rsrc_typ_cd)
select distinct aa.accessor_id , 'MD-1' , '60', 'uid=admin,o=crossroads','uid=admin,o=crossroads', '1'  
