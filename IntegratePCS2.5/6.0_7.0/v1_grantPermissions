	java multiAttrAdd "(&(carrieruid=$companyId)(permissionid=VisPro_PCS^VisPro))" "ou=carriers,o=crossroads" permissionid "PCS_GUI_Co^SUBSCRIBER" 1>>grant$1-$companyId.out 2>>grant$1-$companyId.err;
