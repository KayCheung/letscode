./multiAttrAdd "(&(permissionid=SRS_Top10*^R)(!(permissionid=SRS_Top10_Std^VisSvc^R)))" "ou=people,o=crossroads" permissionid "SRS_Top10_Std^VisSvc^R" 1>>grant$1.out 2>>grant$1.err;
