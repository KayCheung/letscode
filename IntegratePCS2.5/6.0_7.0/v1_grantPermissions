    java $JVMOPTION UpdateAnzCo "(permissionid=${EXISTING_COMP_PERM2})" "ANZ_SA_196_Co" "196" "_EU" "MD-1" SMS 1>>grant$1.out 2>>grant$1.err;
    
    
